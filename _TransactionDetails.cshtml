<div id="transactionModel" class="modal fade" role="dialog" style="z-index:1100">
    <div class="modal-dialog" style="width:850px;">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Transaction Details</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-default panel-primary2">
                            <div class="panel-heading">Account Details</div>
                            <div class="panel-body">
                                <div class="table-container">
                                    <div class="row row-space">
                                        <div class="col-lg-3">
                                            <input type="hidden" id="txtMasterId" value="0" />
                                            <div class="form-group">
                                                <label> Date:</label>
                                                <input type="text" id="txtDate" class="form-control" disabled />
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="form-group">
                                                <label> Deptt./College :</label>
                                                <select id="ddlInstitute" class="form-control ddlInstitute" disabled></select>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="form-group">
                                                <label> Voucher Type:</label>
                                                <select id="ddlVocherTypes" class="form-control" disabled></select>
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="form-group">
                                                <label>Voucher No:</label>
                                                <input type="text" id="txtVoucherNo" class="form-control" disabled />
                                            </div>
                                        </div>
                                        <div class="col-lg-3">
                                            <div class="form-group">
                                                <label>Cheque No./Cash:</label>
                                                <input type="text" id="txtChequeOrCash" class="form-control" disabled />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-default panel-primary2">
                            <div class="panel-heading">Account Ledger</div>
                            <div class="panel-body">
                                <div class="table-container">
                                    <table id="tblNewLedger" class="table table-striped table-bordered table-hover main-table">
                                        <thead>
                                            <tr>
                                                <th>Select</th>
                                                <th>Date</th>
                                                <th>V.Type</th>
                                                <th>V.No</th>
                                                <th>Cheque No./Cash</th>
                                                <th>Cr/Dr</th>
                                                <th>Ledger ID</th>
                                                <th>Ledger Name</th>
                                                <th className="align-right">Debit</th>
                                                <th className="align-right">Credit</th>
                                                <th width="10%">Narration</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                        <tfoot>
                                            <tr>
                                                <th></th>
                                                <th>Total:</th>
                                                <th colspan="6"></th>
                                                <th><span id="lblDebitAmount"></span></th>
                                                <th><span id="lblCreditAmount"></span></th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            Master Narration:
                            <input type="text" id="txtNarration" class="form-control" required />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

